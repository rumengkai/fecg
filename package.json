{
  "name": "fecg",
  "version": "0.0.5",
  "description": "Front end code generator",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel start watch",
    "start": "tsc",
    "build": "tsc",
    "fe": "./dist/index.js",
    "watch": "watch 'npm run copyTpl' src/templates",
    "clean": "rimraf dist/*",
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "copyTpl": "cp -r src/templates/ dist/templates",
    "poststart": "npm run copyTpl && chmod 777 ./dist/index.js && tsc -w",
    "postbuild": "npm run copyTpl && chmod 777 ./dist/index.js"
  },
  "preferGlobal": true,
  "bin": {
    "fe": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "cli",
    "fecg"
  ],
  "author": "rumengkai1",
  "license": "ISC",
  "dependencies": {
    "inquirer": "^7.3.3",
    "openapi3-ts-generator": "^1.0.1-2",
    "plop": "^2.7.6"
  },
  "peerDependencies": {
    "openapi3-ts-generator": "^1.0.1-2",
    "plop": "^2.7.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@types/node": "^17.0.42",
    "eslint": "^8.17.0",
    "typescript": "^4.7.3",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
