{
  "name": "fecg",
  "version": "0.1.9",
  "description": "Front end code generator",
  "homepage": "https://github.com/rumengkai/fecg",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc",
    "build": "tsc",
    "fe": "./dist/index.js",
    "clean": "rimraf dist/*",
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "poststart": "chmod 777 ./dist/index.js && tsc -w",
    "postbuild": "chmod 777 ./dist/index.js && yarn fe -u",
    "test": "cross-env JEST_REPORT_FILE='./test/report/test-report.json'  jest --config test/jest.conf.js"
  },
  "preferGlobal": true,
  "bin": {
    "fe": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rumengkai/fecg.git"
  },
  "keywords": [
    "cli",
    "generator",
    "fecg",
    "fecg-vscode",
    "plop"
  ],
  "author": "rumengkai1",
  "license": "ISC",
  "dependencies": {
    "download-git-repo": "^3.0.2",
    "fs-extra": "^10.1.0",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "openapi3-ts-generator": "1.0.8",
    "ora": "4.0.5",
    "plop": "^2.7.6"
  },
  "peerDependencies": {
    "openapi3-ts-generator": "1.0.8",
    "plop": "^2.7.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@types/node": "^17.0.42",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "jest": "^28.1.2",
    "jest-bamboo-reporter": "^1.2.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.1",
    "jest-html-reporter": "^3.5.0",
    "typescript": "^4.7.3",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
